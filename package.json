{
  "name": "universal-redux-boilerplate",
  "version": "1.0.2",
  "description": "Un simple boilerplate para el uso de universal js con react, redux y react-router",
  "main": "bin/www",
  "scripts": {
    "start:dev": "SERVER=true DEV=true nodemon --exec babel-node --stage 0 -- ./bin/www",
    "start:prod": "SERVER=true PROD=true babel-node ./bin/www",
    "webpack:dev": "DEBUG=true babel-node webpack.server.js",
    "webpack:prod": "webpack --config webpack.config.production.js",
    "test": "jscs . && node ./test/index.js"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "body-parser": "1.14.2",
    "debug": "2.2.0",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "history": "2.0.0-rc2",
    "morgan": "1.6.1",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "2.1.0",
    "redux": "3.0.5",
    "redux-logger": "2.4.0",
    "redux-thunk": "1.0.3",
    "serve-favicon": "2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "babel": "5.8.35",
    "babel-core": "5.8.35",
    "babel-loader": "5.4.0",
    "cheerio": "0.19.0",
    "clean-webpack-plugin": "0.1.7",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "node-libs-browser": "1.0.0",
    "nodemon": "1.8.1",
    "postcss-loader": "0.8.0",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.0.1",
    "redux-devtools-dock-monitor": "1.0.1",
    "redux-devtools-log-monitor": "1.0.2",
    "script-loader": "0.6.1",
    "style-loader": "0.13.0",
    "tape": "4.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.12",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghondar/universal-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-router",
    "react-router-redux",
    "boilerplate",
    "hot",
    "reload",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
  "author": "Ghondar <abgv9221@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghondar/universal-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/ghondar/universal-redux-boilerplate#readme"
}
