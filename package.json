{
  "name": "universal-redux-boilerplate",
  "version": "1.0.6",
  "description": "Un simple boilerplate para el uso de universal js con react, redux y react-router",
  "main": "bin/www",
  "scripts": {
    "start": "npm run webpack:prod && npm run start:prod",
    "start:dev": "SERVER=true DEV=true nodemon ./bin/server.js",
    "start:prod": "NODE_ENV=development SERVER=true PROD=true node ./bin/server.js",
    "webpack:dev": "NODE_ENV=client DEBUG=true node webpack.server.js",
    "webpack:prod": "webpack --config webpack.config.production.js",
    "test": "jscs . && node ./test/index.js"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "autoprefixer": "6.3.7",
    "babel-cli": "6.6.4",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "body-parser": "1.15.2",
    "clean-webpack-plugin": "0.1.9",
    "css-loader": "0.23.1",
    "debug": "2.2.0",
    "express": "4.14.0",
    "express-session": "1.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "history": "3.0.0",
    "json-loader": "0.5.4",
    "morgan": "1.7.0",
    "node-libs-browser": "1.0.0",
    "postcss-loader": "0.9.1",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-redux": "4.0.5",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.10",
    "redux": "3.5.2",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "script-loader": "0.7.0",
    "serve-favicon": "2.3.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-isomorphic-tools": "2.3.2"
  },
  "devDependencies": {
    "cheerio": "0.20.0",
    "nodemon": "1.9.2",
    "tape": "4.6.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghondar/universal-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-router",
    "react-router-redux",
    "boilerplate",
    "hot",
    "reload",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
