{
  "name": "universal-redux-boilerplate",
  "version": "1.0.4",
  "description": "Un simple boilerplate para el uso de universal js con react, redux y react-router",
  "main": "bin/www",
  "scripts": {
    "start": "npm run webpack:prod && npm run start:prod",
    "start:dev": "SERVER=true DEV=true nodemon --exec babel-node -- ./bin/www",
    "start:prod": "SERVER=true PROD=true babel-node ./bin/www",
    "webpack:dev": "NODE_ENV=client DEBUG=true babel-node webpack.server.js",
    "webpack:prod": "webpack --config webpack.config.production.js",
    "test": "jscs . && node ./test/index.js"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "autoprefixer": "6.3.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-loader": "6.2.2",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-runtime": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "1.14.2",
    "clean-webpack-plugin": "0.1.8",
    "css-loader": "0.23.1",
    "debug": "2.2.0",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "file-loader": "0.8.5",
    "history": "2.0.0",
    "json-loader": "0.5.4",
    "morgan": "1.6.1",
    "node-libs-browser": "1.0.0",
    "postcss-loader": "0.8.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.0",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "3.0.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.2",
    "redbox-react": "1.2.2",
    "redux": "3.3.1",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.0.1",
    "redux-devtools-log-monitor": "1.0.4",
    "redux-logger": "2.5.0",
    "redux-thunk": "1.0.3",
    "script-loader": "0.6.1",
    "serve-favicon": "2.3.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.13"
  },
  "devDependencies": {
    "cheerio": "0.20.0",
    "nodemon": "1.8.1",
    "tape": "4.4.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghondar/universal-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-router",
    "react-router-redux",
    "boilerplate",
    "hot",
    "reload",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
  "author": "Ghondar <abgv9221@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghondar/universal-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/ghondar/universal-redux-boilerplate#readme"
}
